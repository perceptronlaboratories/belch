generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Course { 
  id String @id @default(cuid())
  name String 
  season String @default("fall")
  year Int @default(2024)
  documents File[]
}

model File { 
  id String @id @default(cuid())
  url String
  course Course @relation(fields: [courseId], references: [id])
  courseId String
}
